@model IEnumerable<Custom_ORM.Models.User>

<h1>Users</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Date of Birth</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.Name</td>
                    <td>@user.DateOfBirth.ToShortDateString()</td>
                </tr>
        }
    </tbody>
</table>
<a href="@Url.Action("Create")" class="btn btn-primary">Create New User</a>
